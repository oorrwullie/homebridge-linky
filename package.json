{
  "name": "homebridge-linky",
  "version": "0.6.0",
  "description": "A Homebridge plugin to remotely list and control your HomeKit devices over the internet via a secure API key.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "homebridge-plugin",
    "homekit",
    "api",
    "remote",
    "control"
  ],
  "engines": {
    "node": ">=16.0.0",
    "homebridge": ">=1.6.0"
  },
  "homepage": "https://github.com/oorrwullie/homebridge-linky#readme",
  "bugs": {
    "url": "https://github.com/oorrwullie/homebridge-linky/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorrwullie/homebridge-linky.git"
  },
  "author": "Kevin Payne",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "homebridge": "1.6.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "fastify": "^4.26.2",
    "openai": "^4.21.0",
    "simple-git": "^3.22.1"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
